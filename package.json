{
  "name": "dry",
  "version": "0.1.1",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "docker:dev": "cp .env.development docker/development/.env & docker compose -f ./docker/development/docker-compose.yml up --build -d",
    "docker:prod": "cp .env.production docker/production/.env & docker compose -f ./docker/production/docker-compose.yml up --build -d",
    "prisma:generate": "npx prisma generate",
    "prisma:apply": "dotenv -e .env.development -e .env -- npx prisma migrate dev",
    "prisma:seed": "dotenv -e .env.development -e .env -- npx prisma db seed",
    "init:dev": "yarn run docker:dev & yarn run prisma:generate & yarn run prisma:seed",
    "dev": "next dev",
    "build": "yarn run prisma:generate & next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.409.0",
    "@hookform/resolvers": "3.3.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.5.2",
    "autoprefixer": "10.4.15",
    "axios": "1.4.0",
    "bcryptjs": "2.4.3",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "lodash": "4.17.21",
    "next": "13.5.6",
    "next-auth": "4.22.5",
    "next-intl": "2.19.1",
    "node-cache": "5.1.2",
    "postcss": "8.4.30",
    "postcss-import": "15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.1",
    "react-icons": "4.10.1",
    "react-redux": "8.1.2",
    "react-toastify": "9.1.3",
    "react-tooltip": "5.21.1",
    "react-transition-group": "4.4.5",
    "sass": "1.65.1",
    "sharp": "0.32.6",
    "swr": "2.2.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.5.6",
    "@types/bcryptjs": "2.4.5",
    "@types/lodash": "4.14.200",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/react-transition-group": "4.4.5",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.3.0",
    "prettier": "3.0.3",
    "prisma": "5.5.2",
    "ts-node": "10.9.1"
  }
}
