{
  "name": "ravenz",
  "version": "0.1.1",
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "start:local": "node .next/standalone/server.js",
    "start:server": "node server.js",
    "lint": "next lint",
    "prisma:generate": "npx prisma generate",
    "prisma:apply": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:seed": "npx prisma db seed",
    "init:dev": "yarn docker:local & yarn prisma:generate & yarn prisma:seed",
    "docker:local": "cp .env docker/local/.env & docker compose -f ./docker/local/docker-compose.yml up --build -d",
    "docker:dev": "cp .env.staging docker/staging/.env & docker compose -f ./docker/staging/docker-compose.yml up --build -d",
    "docker:server": "cp .env.production docker/production/.env & docker compose -f ./docker/production/docker-compose.yml up --build -d"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.409.0",
    "@hookform/resolvers": "3.3.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@plaiceholder/next": "^3.0.0",
    "@prisma/client": "5.5.2",
    "autoprefixer": "10.4.15",
    "bcryptjs": "2.4.3",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "eslint": "8.52.0",
    "eslint-config-next": "^14.0.3",
    "lodash": "4.17.21",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "next-intl": "3.1.3",
    "node-cache": "5.1.2",
    "plaiceholder": "3.0.0",
    "postcss": "8.4.30",
    "postcss-import": "15.1.0",
    "prisma": "5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.46.1",
    "react-icons": "4.10.1",
    "react-redux": "8.1.2",
    "react-toastify": "9.1.3",
    "react-tooltip": "5.21.1",
    "react-transition-group": "4.4.5",
    "sass": "1.65.1",
    "sharp": "^0.32.6",
    "swr": "2.2.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.3",
    "@types/bcryptjs": "2.4.5",
    "@types/lodash": "4.14.200",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/react-transition-group": "4.4.5",
    "dotenv-cli": "7.3.0",
    "prettier": "3.0.3"
  }
}
